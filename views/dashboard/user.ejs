<!-- dashboard.ejs -->
<% usersData.forEach(user => { %>
    <h2><%= user.name %></h2>
    <% user.notes.forEach(note => { %>
      <div>
        <h3><%= note.title %></h3>
        <p><%= note.description %></p>
        <!-- Display other note properties as needed -->
      </div>
    <% }); %>
  <% }); %>
  